<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Encode</servlet-name>
    <servlet-class>kboss.servlet.Encode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Decode</servlet-name>
    <servlet-class>kboss.servlet.Decode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>kboss.servlet.Main</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>kboss.servlet.Hello</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Encode</servlet-name>
    <url-pattern>/encode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Decode</servlet-name>
    <url-pattern>/decode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>
